{
    "name": "spiderling-php/crawler-curl",
    "description": "A spiderling crawler using simple curl",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Ivan Kerin",
            "email": "ikerin@gmail.com",
            "role": "Author"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/spiderling-php/crawler"
        }
    ],
    "require": {
        "spiderling-php/crawler": "dev-master",
        "spiderling-php/spiderling": "dev-master"
    },
    "require-dev": {
        "guzzlehttp/guzzle" : "^6.1-dev"
    },
    "autoload": {
        "psr-4": {
            "SP\\CrawlerCurl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SP\\CrawlerCurl\\Test\\": "tests/src/",
            "GuzzleHttp\\Tests\\": "vendor/guzzlehttp/guzzle/tests/"
        }
    }
}
